package nueva2;

import nueva.rectangulo;

public class inicio_rectangulo {

	public static void main(String[] args) {
		
		rectangulo r1 =new rectangulo();
		rectangulo r2 =new rectangulo(100);
		rectangulo r3 =new rectangulo(100, 200);
		
		System.out.println(r1 + "perimetro" + r1.getPerimetro());
		System.out.println(r1 + "area" + r1.getArea());
		System.out.println(r2 + "perimetro" + r2.getPerimetro());
		System.out.println(r2 + "area" + r2.getArea());
		
		
		

	}

}
